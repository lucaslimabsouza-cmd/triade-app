{
  "expo": {
    "name": "mobile-2.0",
    "slug": "mobile-2-0",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "triade",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E2A47"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0E2A47"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f6f37165-bd39-4116-bb6b-b2f00f2f3e12"
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
